{
  "name": "ranchlink",
  "version": "0.1.0",
  "private": true,
  "description": "RanchLink - Sophisticated livestock management tag system by Ritxma Integrations LLC",
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "lint": "turbo run lint",
    "type-check": "turbo run type-check",
    "test": "turbo run test",
    "db:migrate": "cd infra/db && supabase migration up",
    "db:seed": "cd infra/db && tsx seed.ts",
    "contracts:compile": "cd packages/contracts && hardhat compile",
    "contracts:deploy": "cd packages/contracts && hardhat run scripts/deploy.ts --network base"
  },
  "devDependencies": {
    "turbo": "^2.1.0",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@8.15.0",
  "dependencies": {
    "@coinbase/cdp-sdk": "^1.38.5",
    "dotenv": "^17.2.3"
  }
}
